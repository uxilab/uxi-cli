{
  "name": "ci-ops",
  "version": "1.0.18",
  "description": "WIP",
  "main": "bin/index.js",
  "bin": {
    "ci-ops": "bin/index.js"
  },
  "commands": {
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watchAll",
    "test:coverage": "node_modules/.bin/jest --coverage",
    "lint": "node_modules/.bin/eslint -c 'node_modules/ci-ops/.eslintrc' ",
    "build": "node_modules/.bin/webpack --config node_modules/ci-ops/webpack.config.js"
  },
  "commands-args": {
    "test": "[<targetPath>]",
    "test:watch": "[<targetPath>]",
    "test:coverage": "[<targetPath>]",
    "lint": "[<targetPath>]",
    "build": "[<sourceFilePath> <targetFilePath>]"
  },
  "scripts": {
    "deploy": "npm version patch && npm publish",
    "build": "webpack"
  },
  "author": "denisflorkin@gmail.com",
  "license": "MIT",
  "@std/esm": {
    "esm": "js",
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "^0.15.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^21.2.0",
    "compression-webpack-plugin": "^1.1.11",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^21.2.1",
    "jest-coverage-ratchet": "^0.2.3",
    "nock": "^9.0.13",
    "shelljs": "^0.7.8",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "jest": {
    "setupFiles": [
      "./shim.js",
      "./jest.conf.js"
    ],
    "verbose": true,
    "testPathIgnorePatterns": [
      "mock/*",
      "<rootDir>/node_modules/*",
      "<rootDir>/test/*"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/testcafe"
    ],
    "globals": {
      "__cluedIn": {
        "token": "oefkeofkeofkeofk"
      }
    },
    "coverageReporters": [
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 53.48,
        "statements": 52.8,
        "functions": 46.24,
        "branches": 27.58
      }
    }
  }
}
